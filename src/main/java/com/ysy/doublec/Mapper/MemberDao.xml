<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ysy.doublec.Mapper.MemberDao">
    <delete id="delMember">
        delete from members where id in
        <foreach collection="deleteIds" index="index" item="item" separator="," open="(" close=")">
            #{item}
        </foreach>
    </delete>
</mapper>